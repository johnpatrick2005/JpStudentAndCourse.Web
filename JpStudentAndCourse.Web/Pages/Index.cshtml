@page
@model IndexModel
@{
    ViewData["Title"] = "Index";
}

<h1>Students</h1>
@foreach (var student in Model.Students)
{
    <h3>@student.Name</h3>
    <p>Email: @student.Email</p>
    <hr />
}

<h1>Courses</h1>
@foreach (var course in Model.Courses)
{
    <h3>@course.Title</h3>
    <p>Credits: @course.Credits</p>
    <hr />
}
